<p><b>Kategorie:</b></p>
<div id="menu">
</div>
<script type='text/x-handlebars-template' id='category-list-template'>
  <a href="#" onclick="useCase.showCategory( {{this.id}} )">
    {{ this.name }}
  </a>
	<br/ >
</script>

<p><b>Produkty:</b></p>
  <div id='products-div'>
  </div>
<script type='text/x-handlebars-template' id='product-list-template'>
  <div class="product">
    {{ this.name }}
    <a href="#" onclick="useCase.showProduct( {{ this.id }} )">Wiecej</a>
  </div>
</script>
<script type='text/x-handlebars-template' id='product-template'>
  <ul>
    <li>Nazwa: {{ this.name }}
    <li>Opis: {{ this.description }}
    <li>Kategoria: {{this.catname}}
    <li>Cena: {{ this.price }}
  </ul>
</script>